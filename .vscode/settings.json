    "editor.mouseWheelZoom": true,
    "editor.snippetSuggestions": "bottom",
    "xliffSync.parseFromDeveloperNote": true,
    "xliffSync.parseFromDeveloperNoteOverwrite": true,
    "al.showHomeAtStartup": "Always",
}